@font-face
  font-family: 'Calluna'
  src: url('/fonts/calluna.eot')
  src: url('/fonts/calluna.eot?#iefix') format('embedded-opentype'),
  url('/fonts/calluna.woff') format('woff'),
  url('/fonts/calluna.ttf') format('truetype')
  font-weight: normal
  font-style: normal

